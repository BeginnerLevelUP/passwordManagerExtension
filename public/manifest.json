{
  "manifest_version": 3,
  "name": "PasswordManager",
  "version": "1.0.0",
  "description": "Meant To Work With PasswordManager",
  "background":{
    "service_worker":"serviceWorker.js",
    "type":"module"
  },
  "action":{
     "default_title": "Click to show an alert",
     "default_popup":"index.html"
  },
    "permissions": [
    "scripting",
    "tabs",
    "activeTab"
  ],
  "host_permissions":[
    "http://*/*",
    "https://*/*"
  ],
    "icons": {
    "16":"icons8-password-manager-16.png",
    "32": "icons8-show-password-32.png",
    "48":"icons8-show-password-48.png",
    "128":"password-manager 128.png"
  }
} 
